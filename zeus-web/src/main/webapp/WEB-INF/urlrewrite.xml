<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite default-match-type="wildcard">
    <rule>
        <from>/**</from>
        <to>/app/$1</to>
    </rule>
    <rule>
        <from>/app/favicon.ico</from>
        <to>/favicon.ico</to>
    </rule>
    <rule>
        <from>/app/temp/**</from>
        <to>/temp/$1</to>
    </rule>
    <rule>
        <from>/app/images/**</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>/app/js/**</from>
        <to>/js/$1</to>
    </rule>
    <rule>
        <from>/app/css/**</from>
        <to>/css/$1</to>
    </rule>
    <rule>
        <from>/app/test/**</from>
        <to>/test/$1</to>
    </rule>
    <rule>
        <from>/app/plugin/**</from>
        <to>/plugin/$1</to>
    </rule>
    <outbound-rule>
        <from>/app/**</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>